{
  "name": "ui-engine-build-scripts",
  "version": "1.0.0",
  "author": "ronnidc & mortenhoffmanndoff",
  "description": "Move scrips below to the www-root package.json",
  "scripts": {
    "setup": "cd ui-engine && npm run setup",
    "dev": "cd ui-engine && npm run dev",
    "generate": "cd ui-engine && npm run generate",
    "build:dev": "cd ui-engine && npm run build:dev",
    "build:prod": "cd ui-engine && npm run build:prod",
    "build:themes": "cd ui-engine && npm run build:themes",
    "build": "cd ui-engine && npm run build",
    "clean": "cd ui-engine && npm run clean",
    "ssr:build": "cd ui-engine && npm run ssr:build",
    "ssr:start": "cd ui-engine && npm run ssr:start"
  }
}
