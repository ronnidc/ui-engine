    "google-maps-api-loader": "1.1.1",
    "import-glob": "1.5.0",
    "mini-css-extract-plugin": "0.9.0",
    "minimist": "1.2.5",
    "node-sass-glob-importer": "5.3.2",
    "postcss-loader": "3.0.0",
    "sass-loader": "8.0.2",
    "string-replace-loader": "2.3.0",
    "url-loader": "4.1.0",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-merge": "4.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.11.4",
    "@babel/preset-env": "7.11.0",
    "@nuxtjs/style-resources": "1.0.0",
    "cssnano": "4.1.10",
    "eslint": "7.7.0",
